{
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "prettier/prettier",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "settings": {
    "import/resolver": {
      "typescript": true,
      "node": true
    }
  },
  "plugins": ["prettier", "import"],
  "rules": {
    "import/no-unused-modules": [1, { "unusedExports": true }]
    // "prettier/prettier": ["warn"]
  },
  "ignorePatterns": ["cmd"]
}
